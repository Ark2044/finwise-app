# FinWise App Environment Variables
# Copy this file to .env and fill in your values

# ===========================================
# NEON AUTH (Stack) - Required for authentication
# Get these from: https://console.neon.tech -> Auth
# ===========================================
EXPO_PUBLIC_STACK_PROJECT_ID=your_neon_auth_project_id
EXPO_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_neon_auth_publishable_key

# ===========================================
# API Configuration
# ===========================================
EXPO_PUBLIC_API_URL=http://localhost:3000

# ===========================================
# Security (Optional - uses default in development)
# ===========================================
EXPO_PUBLIC_ENCRYPTION_KEY=your_secure_encryption_key_32_chars
